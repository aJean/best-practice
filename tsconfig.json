{
    "compilerOptions": {
        "outDir": "./dist/",
        "noImplicitAny": false,
        "module": "esnext",
        "target": "es5",
        "allowJs": true,
        "jsx": "react",
        "downlevelIteration": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "lib": [
            "dom",
            "es2015",
            "esnext.asynciterable"
        ]
    },

    "exclude": [
        "node_modules"
    ]
  }